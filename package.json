{
  "name": "@theharuspex/notion-dispatch-events",
  "version": "0.1.0",
  "description": "Notion Command Central dispatcher and Objective fan-out integration (TypeScript, Express)",
  "main": "dist/exports.js",
  "types": "dist/exports.d.ts",
  "files": ["dist", "README.md"],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "prepublishOnly": "npm run build",
    "start": "node dist/index.js",
    "dev": "ts-node-dev -r dotenv/config --respawn --transpile-only src/index.ts",
    "print:db-props": "node scripts/print-db-props.js"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.17.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  }
}
